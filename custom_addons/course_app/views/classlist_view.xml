<?xml version="1.0"?>
   <odoo>
     <!-- Form view for viewing transcripts -->
     <record id="view_form_classlist" model="ir.ui.view">
       <field name="name">Class List View</field>
       <field name="model">list.module</field>
       <field name="arch" type="xml">
         <form string="Class List">
           <group>
             <field name='l_id'/>
           </group>
           <group>
             <!-- <field name="result_transcript_id" widget="One2many_tags"/>
             <field name="module_transcript_id" widget="One2many_tags"/> -->
           </group>

           <!-- Button that pulls a students list of modules and associated results -->
           <div class="oe_button_box" name="button box"/>
             <button name="open_class_list" string="View class list"
               type="object" class="oe_start_button"/>

         </form>
       </field>
     </record>
</odoo>
